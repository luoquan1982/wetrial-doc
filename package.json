{
  "name": "wetrial-doc",
  "version": "0.0.1",
  "description": "develop doc",
  "keywords": [
    "wetrial develop doc"
  ],
  "authors": {
    "name": "xiexingen",
    "email": "1002275364@qq.com"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": "https://github.com/wetrial/wetrial-doc",
  "homepage": "https://github.com/wetrial/wetrial-doc",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env REACT_APP_ENV=dev father-doc dev",
    "build:doc": "father-doc build",
    "patch": "yarn version --patch && git push origin master && git push origin --tags",
    "minor": "yarn version --minor && git push origin master && git push origin --tags",
    "major": "yarn version --major && git push origin master && git push origin --tags",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && yarn lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0-alpha.19",
    "antd": "^4.0.0-rc.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "peerDependencies": {
    "@ant-design/icons": "*",
    "antd": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@umijs/fabric": "^2.0.2",
    "@umijs/plugin-qiankun": "1.4.0",
    "babel-plugin-import": "^1.12.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.3",
    "del": "^5.1.0",
    "father-doc": "^1.0.0-beta.5",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "umi": "^2.13.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*"
  ],
  "license": "MIT"
}
